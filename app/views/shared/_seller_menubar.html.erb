<nav class="menubar">
    <ul>
      <%= menu_link_to "TOP", :root %>
      <%= form_with(url: '#', method: :get, local: true) do %>
        <%= text_field_tag :q, '', placeholder: 'Search...' %>
        <%= submit_tag 'Search' %>
      <% end %>
      <% if current_user %>
        <ul class="account-menu">
        <%= menu_link_to current_user.full_name + "さん", :account %>
        <%= menu_link_to "ログアウト", :session,
            method: :delete, data: { confirm: "ログアウトしますか？" } %>
        </ul>
      <% else %>
        <%= menu_link_to "ログイン", "#" %>
      <% end %>
      <%= menu_link_to "マイページ", "#" %>
      <%= menu_link_to "出品者TOP", :seller_root %>
      <%= menu_link_to "出品商品管理", :seller_items %>
      <%= menu_link_to "注文管理", :seller_orders %>
      <%= menu_link_to "問い合わせ管理", :rooms %>
    </ul>
</nav>